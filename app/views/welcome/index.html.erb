<h1> Welcome to Viewing Party </h1>
<h3>This app lets you watch movies with friends!</h3>


<%= form_with url: login_path, method: :post, local: true do |f| %>
  <%= f.label :email, "Email:" %>
  <%= f.text_field :email %>
  <%= f.label :username, "Username:" %>
  <%= f.text_field :username %>
  <%= f.label :password, "Password:" %>
  <%= f.password_field :password %>
  <%= f.submit "Sign In" %>
<% end %>

<%= link_to "New to Viewing Party? Register Here", new_user_path %>
